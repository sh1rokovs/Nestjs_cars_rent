{
  "name": "cars-rent",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "engine-version",
    "prestart": "engine-version",
    "prebuild": "engine-version",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/config/orm.config.ts",
    "migration:generate": "nest build && cross-env npm run typeorm -- -d ./src/database/config/orm.config.ts migration:generate ./src/database/migrations/%npm_config_name%_migration",
    "migration:dropAll": "npm run typeorm schema:drop -- -d ./src/database/config/orm.config.ts",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n ./src/database/config/typeorm.config.ts",
    "seed:run": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -r ./dist/database/config -d data-source",
    "db:prepare": "yarn run build && yarn run migration:dropAll && yarn run migration:run && yarn run seed:run"
  },
  "dependencies": {
    "@airhead/nest-typeorm-seeder": "^1.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ngneat/falso": "^6.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^6.6.9",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "engine-version": "^1.0.2",
    "env-cmd": "^10.1.0",
    "faker": "^6.6.6",
    "global": "^4.4.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "save": "^2.9.0",
    "swagger": "^0.7.5",
    "typeorm": "^0.3.16",
    "typeorm-extension": "^2.8.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "^20.1.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16",
    "git": "*",
    "yarn": ">=1.20"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
